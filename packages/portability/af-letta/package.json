{
  "name": "@mastra/portability-af-letta",
  "version": "0.0.1",
  "description": "Letta .af (Agent File) format support for Mastra - enables importing and exporting agents in Letta's portable format",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@mastra/core": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2",
    "vitest": "2.0.3"
  },
  "keywords": [
    "mastra",
    "ai",
    "agents",
    "letta",
    "memgpt",
    "portability",
    "import",
    "export",
    "af-format"
  ],
  "author": "Mastra Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mastra-ai/mastra.git",
    "directory": "packages/portability/af-letta"
  },
  "publishConfig": {
    "access": "public"
  }
}